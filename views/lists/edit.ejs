<!DOCTYPE html>
<html>

<%- include ("../partials/header"); %>

<div class="container">
    <h1>Edit <%= list.name %> </h1>
    <div>
        <form action="/lists/<%= list._id %>?_method=PUT" method="POST">
            <div class="form-group">
                <input class="form-control" type="text" name="list[name]" value="<%= list.name %>">
            </div>
            <div class="form-group">
                <button class="btn btn-primary">Submit!</button>
            </div>
        </form>
        <a href="/lists">Go Back</a>
    </div>
</div>

<%- include ("../partials/footer"); %>